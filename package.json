{
  "name": "@ts-common/source-map",
  "version": "0.3.1",
  "description": "Source Map",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "test": "tsc && nyc mocha",
    "prepack": "npm install && tsc"
  },
  "nyc": {
    "reporter": [
      "html",
      "text",
      "cobertura"
    ],
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.js.map",
    "index.d.ts",
    "index.d.ts.map",
    "index.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ts-common/source-map.git"
  },
  "keywords": [
    "source",
    "map"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ts-common/source-map/issues"
  },
  "homepage": "https://github.com/ts-common/source-map#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@ts-common/iterator": "^0.1.0",
    "@ts-common/json": "^0.1.0",
    "@ts-common/property-set": "^0.0.9",
    "@ts-common/string-map": "^0.2.0"
  }
}
